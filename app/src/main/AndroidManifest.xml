<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.ste.sokoban">

    <application
        android:allowBackup="false"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Game_Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GameRelated_Func_About" />
        <activity android:name=".GameRelated_Base_GameView" />
        <activity android:name=".GameRelated_Base_Maplist" />
        <activity android:name=".GameRelated_Func_NewGame" />
        <activity
            android:name=".GameRelated_Base_OptionHelp"
            android:theme="@android:style/ThemeOverlay.Material.Dialog"
            tools:targetApi="m" />
        <activity
            android:name=".GameRelated_Base_OptionMusic"
            android:theme="@android:style/ThemeOverlay.Material.Dialog"
            tools:targetApi="m" />
        <activity android:name=".GameRelated_Base_OptionMusic_Handle" />

    </application>

</manifest>